<h1>Listing list_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Template</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @list_types.each do |list_type| %>
  <tr>
    <td><%= list_type.name %></td>
    <td><%= list_type.template %></td>
    <td><%= link_to 'Show', list_type %></td>
    <td><%= link_to 'Edit', edit_list_type_path(list_type) %></td>
    <td><%= link_to 'Destroy', list_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

